12 ejercicio dia 9

# 1. Inicialización: crear los sets con habilidades iniciales
frontend = {'HTML', 'CSS', 'JavaScript', 'React'}
backend = {'Python', 'SQL', 'JavaScript', 'Django'}

frontend.add('Vue.js')


backend.discard('Django')  # uso discard 
# Unión: todas las habilidades únicas en la empresa
habilidades_unicas = frontend.union(backend)
# Intersección: habilidades que tienen en común ambos equipos
habilidades_comunes = frontend.intersection(backend)
# Diferencia: habilidades exclusivas del equipo frontend
habilidades_exclusivas_frontend = frontend.difference(backend)

# Mostrar resultados
print("Habilidades únicas en la empresa:", habilidades_unicas)
print("Habilidades comunes a ambos equipos:", habilidades_comunes)
print("Habilidades exclusivas del equipo Frontend:", habilidades_exclusivas_frontend)


